@model List<Equipment>

@{
    ViewData["Title"] = "Shop";
}

<div class="shop-div">
    <div class="shop-box">
        <div class="shop-slot-box">
            @foreach (Equipment equipment in Model)
            {
                <div class="shop-slot">
                    <img src="~/images/Equipment/@equipment.Image" />
                </div>
            }
        </div>
    </div>
</div>
